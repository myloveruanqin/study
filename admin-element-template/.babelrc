{
  "presets": [
    ["env", { "modules": false }],
    ["es2015", { "loose": true, "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "syntax-dynamic-import",
    [
      "component",
      [
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ],
    "transform-flow-strip-types"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
